{
  "name": "@firemix/admin",
  "version": "0.1.15",
  "description": "Firemix SDK targeting Firebase Admin usage.",
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "type": "commonjs",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@firemix/core": "^0.1.3"
  },
  "peerDependencies": {
    "firebase-admin": "^13.6.0"
  },
  "devDependencies": {
    "firebase-admin": "^13.6.0"
  }
}
