{
  "name": "@firemix/core",
  "version": "0.1.13",
  "description": "Core Firemix types and abstractions shared by Firemix SDK variants.",
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b . && tsc -b tsconfig.cjs.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "rxjs": "^7.8.1"
  }
}
