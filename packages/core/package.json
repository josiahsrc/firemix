{
  "name": "@firemix/core",
  "version": "0.1.14",
  "description": "Core Firemix types and abstractions shared by Firemix SDK variants.",
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "type": "commonjs",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "rxjs": "^7.8.1"
  }
}
