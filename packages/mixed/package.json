{
  "name": "@firemix/mixed",
  "version": "0.1.9",
  "description": "Firemix SDK bundling both client and admin Firebase strategies with zone support.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "license": "ISC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b . && tsc -b tsconfig.cjs.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@firemix/admin": "^0.1.3",
    "@firemix/client": "^0.1.3",
    "@firemix/core": "^0.1.3"
  },
  "peerDependencies": {
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2"
  },
  "devDependencies": {
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2"
  }
}
